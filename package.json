{
  "name": "reminder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/nunito-sans": "^4.5.10",
    "@fontsource/roboto": "^5.0.8",
    "@fontsource/source-serif-pro": "^5.0.5",
    "@google-pay/button-react": "^3.0.10",
    "@mui/icons-material": "^5.14.12",
    "@mui/material": "^5.14.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.14",
    "axios": "^1.5.1",
    "date-fns": "^2.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-date-picker": "^10.5.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.16.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "use-immer": "^0.9.0",
    "validator": "^13.9.0",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI= react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" --fix",
    "test:ci": "npm run test -- --ci --watchAll=false --reporters=default --reporters=jest-junit --reporters=jest-sonar --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-restricted-syntax": [
        "error",
        {
          "selector": ":not(BinaryExpression:matches([operator='!=='], [operator='==='])) > Literal[value=null]",
          "message": "Usage of \"null\" is deprecated in this project, except when received from legacy APIs; use \"undefined\" instead."
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.tsx"
        ],
        "rules": {
          "no-restricted-syntax": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/react": "^14.0.0",
    "jest": "^27.5.1"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
